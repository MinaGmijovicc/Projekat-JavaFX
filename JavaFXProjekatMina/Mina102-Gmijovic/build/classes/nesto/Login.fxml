<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="239.0" prefWidth="366.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nesto.LoginController">
   <children>
      <Label layoutX="45.0" layoutY="72.0" text="Username:">
         <font>
            <Font name="Calibri" size="14.0" />
         </font>
      </Label>
      <Label layoutX="45.0" layoutY="122.0" text="Password:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="txtusername" layoutX="118.0" layoutY="68.0" promptText="username">
         <font>
            <Font name="System Italic" size="12.0" />
         </font></TextField>
      <Button fx:id="btnlogin" layoutX="144.0" layoutY="173.0" mnemonicParsing="false" onAction="#btnloginclick" text="Uloguj se">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <PasswordField fx:id="txtpassword" layoutX="118.0" layoutY="120.0" promptText="password">
         <font>
            <Font name="System Italic" size="12.0" />
         </font></PasswordField>
   </children>
</AnchorPane>
